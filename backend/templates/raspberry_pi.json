{
  "name": "树莓派配置",
  "description": "适用于树莓派系列设备的OpenWrt配置",
  "target": "bcm27xx/bcm2711",
  "config": {
    "CONFIG_TARGET_bcm27xx": true,
    "CONFIG_TARGET_bcm27xx_bcm2711": true,
    "CONFIG_TARGET_bcm27xx_bcm2711_DEVICE_rpi-4": true,
    "CONFIG_PACKAGE_luci": true,
    "CONFIG_PACKAGE_luci-ssl": true,
    "CONFIG_PACKAGE_luci-app-firewall": true,
    "CONFIG_PACKAGE_luci-app-opkg": true,
    "CONFIG_PACKAGE_luci-theme-bootstrap": true,
    "CONFIG_PACKAGE_dnsmasq": false,
    "CONFIG_PACKAGE_dnsmasq-full": true,
    "CONFIG_PACKAGE_ipv6helper": true,
    "CONFIG_PACKAGE_ppp": true,
    "CONFIG_PACKAGE_ppp-mod-pppoe": true,
    "CONFIG_PACKAGE_kmod-usb-ohci": true,
    "CONFIG_PACKAGE_kmod-usb-uhci": true,
    "CONFIG_PACKAGE_kmod-usb2": true,
    "CONFIG_PACKAGE_kmod-usb3": true,
    "CONFIG_PACKAGE_kmod-usb-storage": true,
    "CONFIG_PACKAGE_kmod-fs-ext4": true,
    "CONFIG_PACKAGE_kmod-fs-vfat": true,
    "CONFIG_PACKAGE_block-mount": true,
    "CONFIG_PACKAGE_e2fsprogs": true,
    "CONFIG_PACKAGE_fdisk": true,
    "CONFIG_PACKAGE_wget-ssl": true,
    "CONFIG_PACKAGE_curl": true,
    "CONFIG_PACKAGE_htop": true,
    "CONFIG_PACKAGE_nano": true,
    "CONFIG_PACKAGE_openssh-sftp-server": true,
    "CONFIG_PACKAGE_wpad-basic": false,
    "CONFIG_PACKAGE_wpad-openssl": true,
    "CONFIG_PACKAGE_kmod-brcmfmac": true,
    "CONFIG_PACKAGE_kmod-cfg80211": true,
    "CONFIG_PACKAGE_wireless-tools": true,
    "CONFIG_PACKAGE_iw": true,
    "CONFIG_PACKAGE_hostapd-common": true,
    "CONFIG_PACKAGE_kmod-i2c-core": true,
    "CONFIG_PACKAGE_kmod-i2c-bcm2835": true,
    "CONFIG_PACKAGE_kmod-spi-bcm2835": true,
    "CONFIG_PACKAGE_kmod-sound-core": true,
    "CONFIG_PACKAGE_kmod-sound-arm-bcm2835": true,
    "CONFIG_PACKAGE_python3": true,
    "CONFIG_PACKAGE_python3-pip": true,
    "CONFIG_TARGET_KERNEL_PARTSIZE": 32,
    "CONFIG_TARGET_ROOTFS_PARTSIZE": 512
  },
  "features": [
    "Web管理界面 (LuCI)",
    "SSL支持",
    "防火墙管理",
    "WiFi支持",
    "USB存储支持",
    "I2C/SPI支持",
    "音频支持",
    "Python3环境",
    "SSH/SFTP服务"
  ]
}
